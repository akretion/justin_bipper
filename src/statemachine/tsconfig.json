{
  "raph": {
    "moduleResolution": "node",
    "module": "commonjs"
},
    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "isolatedModules": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "noImplicitUseStrict": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "./dist/"
    },
    "files": [
      "./src/states.ts"
    ],
    "exclude": [
        "node_modules",
        "typings/browser",
        "typings/browser.d.ts",
        "bower_components",
        "test",
        "dist"
    ],
    "compileOnSave": true,
    "buildOnSave": false,
    "atom": {
        "rewriteTsconfig": false
    }
}
